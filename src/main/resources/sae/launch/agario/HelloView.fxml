<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox style="-fx-alignment: center;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sae.launch.agario.controllers.HelloController">
   <children>
      <Label text="Bienvenu sur notre jeux AgarIo">
         <VBox.margin>
            <Insets bottom="50.0" />
         </VBox.margin>
      </Label>
   
             <Label fx:id="welcomeText" />
       <Button onAction="#onLocalButtonClick" text="Jouer en local">
         <VBox.margin>
            <Insets bottom="10.0" top="20.0" />
         </VBox.margin></Button>
      <Button onAction="#onMultiButtonClick" text="Jouer en Multijoueur" />
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
</VBox>
